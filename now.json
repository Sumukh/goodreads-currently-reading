{
    "version": 2,
    "name": "goodreads-reading",
    "routes": [
        { "src": "/.*", "dest": "/"}
    ],
    "builds": [
        {
            "src": "index.py",
            "use": "@now/python"
        }
    ],
    "build": {
        "env": {
            "GOODREADS_API_KEY": "@goodreads-key"
        }
    }
}